{
  "build": {
    "content": [
      {
        "files": [
          "pro/api/help/**.yml",
          "pro/api/help/**.md",
          "pro/api/shipments/**.yml",
          "pro/api/shipments/**.md",
          "pro/how-to/**.md",
          "pro/how-to/**.yml",
          "pro/toc.yml",
          "pro/index.md"
        ]
      },
      {
        "files": [
          "react/**.md",
          "react/**.yml",
          "react/help/**.md",
          "react/help/**.yml"
        ]
      },
      {
        "files": [
          "pro/reports/**.md",
          "pro/reports/toc.yml",
          "pro/release-notes/**.md",
          "pro/release-notes/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "pro/reports/**.md",
          "pro/reports/toc.yml",
          "pro/overview/**.md",
          "pro/overview/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "pro/images/**",
          "pro/how-to/images/**",
          "react/help/images/**",
          "images/**"
        ]
      },
      {
        "files": [
          "pro/scripts/**"
        ]
      },
      {
        "files": [
          "myfavicon.ico"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "pro/apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/sorted"
    ],
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appFooter": "<span>Copyright Â© Sorted Group 2020. Generated by <strong>DocFX</strong></span>",
      "_appTitle": "Sorted Docs",
      "_enableSearch": true,
      "_gitContribute": {
        "branch": "content/shipments"
      }
    }
  }
}