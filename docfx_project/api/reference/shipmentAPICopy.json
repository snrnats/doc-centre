            "required": false,
            "type": "string",
            "collectionFormat": "multi",
            "uniqueItems": false
          }
        ],
        "responses": {
          "200": {
            "description": "A shipment object was returned successfully",
            "schema":{ 
              "$ref":"#/definitions/GetLabelsResponse"
           },
            "examples": {
              "application/json": {
                "$ref":"#/definitions/GetLabelsResponse"
              }  
            }  
          }
        }
      }
    },
