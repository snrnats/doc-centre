{{!master(layout/_master.tmpl)}}
{{{rawTitle}}}
<div class="page-stats row visible-xs">
    <ul class="page-metadata">
        <li class="li-page-date">
            {{#gitPageDate}}{{gitPageDate}}{{/gitPageDate}}
        </li>
        <li class="li-time-to-read">
            {{#wordCount}} {{timeToRead}} {{/wordCount}} to read
        </li>
        <li class="contributors">
            {{#gitPageLink}}
            <a href="{{gitPageLink}}" target="_blank">
                <ul class="contributor-list">
                    {{#gitPageContributor1Login}}
                        <li class="contributor-li">
                            <img class="img-circle img-contributor" src="{{gitPageContributor1AvatarUrl}}" alt="{{gitPageContributor1Login}}">
                        </li>
                    {{/gitPageContributor1Login}}
                    {{#gitPageContributor2Login}}
                        <li class="contributor-li">
                            <img class="img-circle img-contributor" src="{{gitPageContributor2AvatarUrl}}" alt="{{gitPageContributor2Login}}">
                        </li>
                    {{/gitPageContributor2Login}}
                    {{#gitPageContributor3Login}}
                        <li class="contributor-li">
                            <img class="img-circle img-contributor" src="{{gitPageContributor3AvatarUrl}}" alt="{{gitPageContributor3Login}}">
                        </li>
                    {{/gitPageContributor3Login}}
                    {{#gitPageContributor4Login}}
                        <li class="contributor-li">
                            <img class="img-circle img-contributor" src="{{gitPageContributor4AvatarUrl}}" alt="{{gitPageContributor4Login}}">
                        </li>
                    {{/gitPageContributor4Login}}
                    {{#gitPageContributor5Login}}
                        <li class="contributor-li">
                            <img class="img-circle img-contributor" src="{{gitPageContributor5AvatarUrl}}" alt="{{gitPageContributor5Login}}">
                        </li>
                    {{/gitPageContributor5Login}}
                </ul>
            </a>
            {{/gitPageLink}}
        </li>
    </ul>
</div>
